  "main": "index.js",
  "scripts": {
    "sass": "sass --watch ./src/app/styles/index.scss config/storybook/main.css",
    "start": "webpack serve --env port=3000",
    "vite": "vite",
    "start:dev": "concurrently \"yarn start --env mode=development\" \"yarn start:dev:server\"",
    "start:vite": "concurrently \"yarn vite\" \"yarn start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "webpack": "webpack",
    "build:prod": "webpack --env mode=production apiUrl=https://jsonserver-futurecatforsale.b4a.run/",
    "build:dev": "webpack --env mode=development",
    "prettier": "npx prettier -- write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" --quiet",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test": "jest",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "brew install graphicsmagick && npx loki --requireReference update --reactUri file:./storybook-static",
    "test:ui:report:npm": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:report:yarn": "yarn test:ui:json && yarn test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky install",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "generate:ui": "node ./scripts/createSlice/templates/createUI.js",
    "postinstall": "rm -rf ./node_modules/.cache",
    "build-storybook": "storybook build",
    "remove-feature": "ts-node ./scripts/remove-feature.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
